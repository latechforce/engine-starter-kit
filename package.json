    "prettier": "^3.5.2",
    "typescript-eslint": "^8.24.1"
  },
  "peerDependencies": {
    "typescript": "^5.7.3"
  },
  "dependencies": {
    "@latechforce/engine": "latest"
  },
  "engines": {
    "bun": "1.2.3"
  }
}
